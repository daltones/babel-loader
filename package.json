  },
  "peerDependencies": {
    "babel-core": "^6.0.0",
    "webpack": "1 || ^2.1.0-beta"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^16.0.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-latest": "^6.16.0",
    "codecov": "^1.0.1",
    "cross-env": "^2.0.0",
    "eslint": "^3.8.1",
    "eslint-config-babel": "^2.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.25.0",
